{
  "schemaVersion": 1,
  "id": "${MODID}",
  "name": "${NAME}",
  "version": "${version}",
  "environment": "client",
  "icon": "icon.png",
  "authors": ["${AUTHOR}"],
  "license": "MIT",
  "contact": {
    "homepage": "${CURSE_HOMEPAGE}",
    "sources": "${GIT_REPO}",
    "issues": "${GIT_REPO}/issues"
  },
  "description": "${DESCRIPTION}",
  "entrypoints": {
    "client": [
      "com.blamejared.bigshot.BigShotFabric"
    ]
  },
  "depends": {
    "fabricloader": ">=${FABRIC_LOADER}",
    "java": ">=${JAVA}",
    "minecraft": "${MINECRAFT}"
  },
  "recommends": {
    "fabric": "*"
  },
  "mixins": [
    {
      "config": "bigshot.mixins.json",
      "environment": "client"
    },
    {
      "config": "bigshot.fabric.mixins.json",
      "environment": "client"
    }
  ]
}
